<div class=" wrapper wrapper-content animated fadeInRight" ng-hide="ShowVendorInfo==0">
    <div class="row">

        <div class="col-lg-12" style="top:-80px;">
            <button class="btn btn-lg btn-outline btn-warning" ng-click="VendorList()" style="margin-bottom:20px">Back</button>

            <div class="ibox product-detail">
                <div class="ibox-content product-box" style="padding-left:0px;padding-right:0px;">

                    <div class="row">
                        <div class="col-md-5" style="padding-left:30px;">


                            <div class="product-images slick-initialized slick-slider" role="toolbar" style="margin-left:0px;margin-right:0px;">

                                <button class="btn btn-lg btn-outline btn-warning btn-rounded" ng-click="VendorList()" style="margin-bottom:20px;position:absolute;bottom:333px"><span class="fa fa-arrow-left"></span></button>

                                <img ng-src="{{VendorDetails.FilePath}}" style="padding-top:20px;height:370px;width:100%;" />


                            </div>

                        </div>
                        <div class="col-md-7">

                            <h2 class="font-bold m-b-xs">
                                {{VendorDetails.CompanyName}}
                            </h2>
                            <small>{{VendorDetails.ServiceName}}</small>
                            <div style="font-size:45px;">
                                <span ng-class="{'fa fa-star checked': OverallRating >= 1,'fa fa-star ':  OverallRating < 1}"></span>
                                <span ng-class="{'fa fa-star checked': OverallRating >= 2,'fa fa-star ':  OverallRating < 2}"></span>
                                <span ng-class="{'fa fa-star checked': OverallRating >= 3,'fa fa-star ':  OverallRating < 3}"></span>
                                <span ng-class="{'fa fa-star checked': OverallRating >= 4,'fa fa-star ':  OverallRating < 4}"></span>
                                <span ng-class="{'fa fa-star checked': OverallRating >= 5,'fa fa-star ':  OverallRating < 5}"></span>
                            </div>

                            <div class="m-t-md" style="margin-top:0px;">
                                <h4 style="margin-top:0px;">Vendor Price </h4>
                                <h2 class="product-main-price"> ₱{{VendorDetails.StartPrice}} - ₱{{VendorDetails.EndPrice}}</h2>
                            </div>
                            <hr>

                            <h4>Vendor description</h4>

                            <div class="small text-muted">

                                {{VendorDetails.Sypnosis}}
                            </div>
                            <div class="small m-t-md">

                                <a target="_self" ng-href="{{VendorDetails.FilePath}}" class="btn btn-md btn-outline btn-rounded btn-warning pull-right" title="Company Logo" style="margin-right:20px;" data-gallery><span class="fa fa-picture-o"></span>  View Gallery</a>
                                <a ng-repeat="img in Img track by $index" target="_self" ng-href="{{img}}" data-gallery="" title="Sample work/service"></a>


                                <span class="fa fa-map-marker"></span>     {{VendorDetails.Province}},{{VendorDetails.City}}




                                <div>
                                    Contact us <b>{{VendorDetails.ContactNo}}</b>
                                </div>

                            </div>
                            <hr>

                            <div>
                                <div class="btn-group">
                                    <button class="btn btn-primary btn-sm"><i class="fa fa-cart-plus"></i> Request Booking</button>
                                    <button class="btn btn-white btn-sm" ng-click="setBookmark(VendorDetails)" ng-style="{'color':(isBookmarked==1)?'#544d44':'','background-color':(isBookmarked==1)?'#ffe800':''}"><i class="fa fa-bookmark"></i> <i style="font-style:normal" ng-hide="isBookmarked==1">Bookmark</i> <i style="font-style:normal;font-weight:bold" ng-hide="isBookmarked==0">Bookmarked</i></button>
                                    <button class="btn btn-white btn-sm" data-toggle="modal" data-target="#SendMessage"><i class="fa fa-envelope"></i> Send Message </button>
                                    <button class="btn btn-white btn-sm" data-toggle="modal" data-target="#ReviewRate"><i class="fa fa-star"></i> Rate Company</button>
                                </div>
                            </div>



                        </div>
                    </div>

                </div>
                <div class="ibox-footer">
                    <div style="font-size:30px;color:#c7a519;margin-bottom:20px;">
                        <span class="fa fa-thumbs-up" style="padding-left:15px;"></span><strong style="padding-left:10px;">REVIEWS</strong>
                    </div>
                    <div style="text-align:center;font-style:italic;font-size:20px;color:#101010a8" ng-show="RatingList.length==0">No Reviews Yet</div>
                    <div class="feed-activity-list" ng-repeat="rates in RatingList|orderBy:'-'">
                        <div class="feed-element" style="margin-bottom: 10px;border-bottom-width:0px;">
                            <div class="pull-left">
                                <img alt="image" class="img-circle" ng-src="{{rates.userprofpic}}">
                            </div>
                            <div class="media-body ">
                                <small class="pull-right">{{rates.DateRated }}</small>
                                Rate by <strong>{{rates.userlname}} {{rates.userfname.slice(0,1)}}</strong> . <br>

                                <small class="text-muted inline">
                                    <span ng-class="{'fa fa-star checked': rates.Rating >= 1,'fa fa-star ': rates.Rating < 1}"></span>
                                    <span ng-class="{'fa fa-star checked': rates.Rating >= 2,'fa fa-star ': rates.Rating < 2}"></span>
                                    <span ng-class="{'fa fa-star checked': rates.Rating >= 3,'fa fa-star ': rates.Rating < 3}"></span>
                                    <span ng-class="{'fa fa-star checked': rates.Rating >= 4,'fa fa-star ': rates.Rating < 4}"></span>
                                    <span ng-class="{'fa fa-star checked': rates.Rating >= 5,'fa fa-star ': rates.Rating < 5}"></span>
                                </small>
                                <div class="well">
                                    {{rates.Comment}}
                                </div>

                            </div>
                        </div>





                    </div>

                </div>
            </div>

        </div>
    </div>
</div>
<div id="blueimp-gallery" class="blueimp-gallery">
    <div class="slides"></div>
    <h3 class="title"></h3>
    <a class="prev">‹</a>
    <a class="next">›</a>
    <a class="close">×</a>

    <ol class="indicator"></ol>
</div>

<div class="modal inmodal" id="ReviewRate" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated bounceIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <i class="fa fa-heart-o modal-icon"></i>
                <h4 class="modal-title">Rate Company</h4>
                <small class="font-bold">Rate and express your opinion of this company</small>
            </div>
            <div class="modal-body">
                <p>
                    <textarea rows="10" ng-required="" ng-model="r.Comment" style="width:100%;display:block;" placeholder="Your comments and opinions regarding the service, products, or performance of the Company"></textarea>
                </p>
                <div class="form-group" style="margin-bottom:0px;"><label style="margin-bottom:0px;font-size:18px;">Rating</label> </div>
                <div>
                    <span ng-repeat="rate in rates" ng-model="r.Rating" ng-class="{'fa fa-star checked clickable': rate.rate <= rateNum,'fa fa-star clickable':rate.rate>rateNum}" ng-mouseover="Starhover(rate)" ng-mouseleave="Starleave(rate)" ng-click="Starclick(rate)" style="font-size:30px;"></span>
                    <span style="font-size:30px;padding-left:15px;">{{rates[rateNum].name}}</span>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" ng-click="SaveRating(r)">Send Review</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal inmodal" id="SendMessage" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated bounceIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <i class="fa fa-envelope modal-icon"></i>
                <h4 class="modal-title">Send Message</h4>
                <small class="font-bold">Start conversation with <bold>{{VendorDetails.CompanyName}}</bold>. Ask questions and inquiries</small>
            </div>
            <div class="modal-body">
                <p>
                    <textarea rows="10" ng-required="" ng-model="msg" style="width:100%;display:block;" placeholder="Ask question for more details"></textarea>
                </p>
                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" ng-click="SendMessage(msg)">Send Message</button>
                </div>
            </div>
        </div>
    </div>
</div>
