<script type="text/javascript">
    $(function () {//on document load
      
        $('#WeddingForm').hide();
        $('#BirthdayForm').hide();
        $('#ReunionForm').hide();
        $('#OtherForm').hide();

        $('#sel1').change(function () {
            showForm($(this).val());
        });

    });

    $(function () {//on document load
        $('#ClassReunion').hide();

        $('#sel2').change(function () {
            showFormReunion($(this).val());
        });

    });

    for (var i = 1980; i <= 2020; i++) {
        $('#classYear').append($('<option>', {
            value: i,
            text: i
        }));
    }

    function showForm(myFormType) {
        if (myFormType == 'Wedding') {
            $('#BirthdayForm').hide();
            $('#BirthdayForm :input').prop('required', false);
            $('#ReunionForm').hide();
            $('#ReunionForm :input').prop('required', false);
            $('#OtherForm').hide();
            $('#OtherForm :input').prop('required', false);
            $('#WeddingForm :input').prop('required', true);
            $('#WeddingForm').show();


        }
        else if (myFormType == 'Birthday Party') {
            $('#WeddingForm').hide();
            $('#WeddingForm :input').prop('required', false);
            $('#ReunionForm').hide();
            $('#ReunionForm :input').prop('required', false);
            $('#OtherForm').hide();
            $('#OtherForm :input').prop('required', false);
            $('#BirthdayForm :input').prop('required', true);
            $('#BirthdayForm').show();

        }
        else if (myFormType == 'Reunions') {
            $('#BirthdayForm').hide();
            $('#BirthdayForm :input').prop('required', false);
            $('#WeddingForm').hide();
            $('#WeddingForm :input').prop('required', false);
            $('#OtherForm').hide();
            $('#OtherForm :input').prop('required', false);
            $('#ReunionForm :input').prop('required', true);
            $('#ReunionForm').show();
        }
        else if (myFormType == 'Other Parties...') {
            $('#BirthdayForm').hide();
            $('#BirthdayForm :input').prop('required', false);
            $('#WeddingForm').hide();
            $('#WeddingForm :input').prop('required', false);
            $('#ReunionForm').hide();
            $('#ReunionForm :input').prop('required', false);
            $('#OtherForm :input').prop('required', true);
            $('#OtherForm').show();
        }

    }

    function showFormReunion(ReunionType) {
        if (ReunionType == 'Class Reunion') {
            $('#FamReunion').hide();
            $('#FamReunion :input').prop('required', false);
            $('#ClassReunion :input').prop('required', true);
            $('#ClassReunion').show();
        }
        else {
            $('#FamReunion :input').prop('required', true);
            $('#FamReunion').show();
            $('#ClassReunion :input').prop('required', false);
            $('#ClassReunion').hide();
        }
    }


    $(document).on('click', '.panel-heading a', function (e) {
        var $this = $(this);
        if ($this.hasClass('collapsed')) {


            $this.find('span').removeClass('fa fa-chevron-up pull-right').addClass('fa fa-chevron-down pull-right');

        } else {
            $this.parents('.panel').find('.panel-body').slideDown();

            $this.find('span').removeClass('fa fa-chevron-down pull-right').addClass('fa fa-chevron-up pull-right');

        }
    })
    

    
   
</script>



























